<div class="page-header">
  <%= link_to entries_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_entry_path(@entry), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show entry</h1>
</div>

<dl class="dl-horizontal">
  <dt>Date:</dt>
  <dd><%= @entry.date %></dd>

  <dt>Minutes:</dt>
  <dd><%= @entry.minutes %></dd>

  <dt>Project:</dt>
  <dd><%= @entry.project %></dd>

  <dt>Description:</dt>
  <dd><%= @entry.description %></dd>

  <dt>Journal:</dt>
  <dd><%= @entry.journal %></dd>

  <dt>Billable:</dt>
  <dd><%= @entry.billable %></dd>

</dl>
